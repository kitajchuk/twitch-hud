{
    "name": "twitch-hud",
    "productName": "Twitch Hud",
    "description": "Custom Twitch HUD for @kitajchuk.",
    "version": "1.0.0",
    "author": "Brandon Lee Kitajchuk",
    "private": true,
    "main": "hud/main.js",
    "scripts": {
        "hub:boot": "node ./hub/boot.js",
        "hub:start": "./node_modules/.bin/forever -a -l forever.log -o out.log -e err.log start ./hub/main.js",
        "hub:start:dev": "./node_modules/.bin/nodemon -i ./hud/source -i ./hud/public -i node_modules ./hub/main.js -- 'dev'",
        "hub:stop": "./node_modules/.bin/forever stopall",
        "hub:deploy": "rsync -av -e 'ssh -i /Users/kitajchuk/.ssh/clutch.pem' --rsync-path 'sudo rsync' hub config.js files.js package.json ec2-user@35.165.160.198:/var/www/html/",
        "hub:connect": "ssh ec2-user@35.165.160.198 -i ~/.ssh/clutch.pem",
        "hud:start": "./node_modules/.bin/nodemon -i source -i public -i node_modules ./hud/main.js",
        "hud:start:dev": "./node_modules/.bin/nodemon -i source -i public -i node_modules ./hud/main.js -- 'dev'",
        "hud:build": "./node_modules/.bin/webpack --env.production --progress --profile --display-modules --colors -p",
        "hud:watch": "./node_modules/.bin/webpack --env.sandbox --hide-modules --colors --watch",
        "postinstall": "cd ./node_modules/properjs-hobo && npm install && npm run build -- 'is eq not one next prev attr last first index parent filter detach append remove trigger prepend closest children removeAttr toggleClass'"
    },
    "repository": "git@github.com:kitajchuk/twitch-hud.git",
    "license": "CC0-1.0",
    "contributors": [
        {
            "name": "Brandon Lee Kitajchuk",
            "email": "kitajchuk@gmail.com",
            "url": "http://kitajchuk.com"
        }
    ],
    "readmeFilename": "README.md",
    "keywords": [],
    "devDependencies": {},
    "dependencies": {
        "autoprefixer": "^6.7.6",
        "babel-core": "^6.23.1",
        "babel-loader": "^6.3.2",
        "babel-preset-es2015": "^6.22.0",
        "body-parser": "^1.18.2",
        "cookie-parser": "^1.4.3",
        "crypto": "^1.0.1",
        "css-loader": "^0.26.2",
        "ejs": "^2.5.7",
        "eslint": "^3.17.0",
        "eslint-loader": "^1.6.3",
        "expose-loader": "^0.7.3",
        "express": "^4.15.3",
        "file-loader": "^0.10.1",
        "forever": "^0.15.2",
        "node-sass": "^4.5.0",
        "nodemon": "^1.14.11",
        "paramalama": "~0.2",
        "postcss": "^5.2.15",
        "postcss-cssnext": "^2.9.0",
        "postcss-import": "^9.1.0",
        "postcss-loader": "^1.3.3",
        "properjs-controller": "~0.2",
        "properjs-debounce": "^0.2.0",
        "properjs-easing": "~0.2",
        "properjs-hobo": "^0.3.10",
        "properjs-imageloader": ">=0.3.1",
        "properjs-lager": "^0.1.2",
        "properjs-mediabox": "~0.3",
        "properjs-pagecontroller": "~0.4",
        "properjs-resizecontroller": "~0.2",
        "properjs-router": "^0.3.7",
        "properjs-scroll2": "~0.2",
        "properjs-scrollcontroller": "~0.2",
        "properjs-slacker": "^0.1.4",
        "properjs-template": "~0.2",
        "properjs-throttle": "^0.3.0",
        "properjs-tween": "~0.2",
        "request": "^2.81.0",
        "request-promise": "^4.2.2",
        "sass-loader": "^6.0.2",
        "tmi.js": "^1.2.1",
        "twitch-helix": "^1.0.7",
        "twitch-webhook": "^1.2.4",
        "webpack": "^2.2.1",
        "websocket": "^1.0.24"
    }
}
